{
  "name": "stardew-valley-checklist",
  "version": "1.2.0",
  "description": "Interactive checklist application for tracking Stardew Valley Community Center bundles completion",
  "type": "module",
  "author": {
    "name": "DariaBrusnitsina",
    "url": "https://github.com/DariaBrusnitsina"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DariaBrusnitsina/stardew-valley-checklist.git"
  },
  "homepage": "https://dariabrusnitsina.github.io/stardew-valley-checklist",
  "bugs": {
    "url": "https://github.com/DariaBrusnitsina/stardew-valley-checklist/issues"
  },
  "keywords": [
    "stardew-valley",
    "checklist",
    "community-center",
    "bundles",
    "react",
    "typescript",
    "vite",
    "game-tracker"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "format:check": "prettier --check 'src/**/*.{ts,tsx,scss,css,json}'",
    "type-check": "tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.2",
    "i18next-http-backend": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^15.4.0",
    "react-router-dom": "^7.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.83.4",
    "stylelint-config-prettier": "^9.0.5",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5"
  }
}
